{
  "version": 2,
  "routes": [
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/script.js",
      "dest": "/script.js"
    },
    {
      "src": "/config.js",
      "dest": "/config.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 